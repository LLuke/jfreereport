<?xml version="1.0" encoding="iso-8859-1"?>
<!--
        <!DOCTYPE report SYSTEM "http://jfreereport.sourceforge.net/report.dtd">
 -->
<report name="First Report" width="468" height="648">

    <reportheader height="24" fontname="Serif" fontstyle="bold" fontsize="20">
        <label name="label1" x="0" y="0" width="100%" height="24" alignment="center" baseline="20">Java Icons</label>
    </reportheader>

    <reportfooter height="48" fontname="Serif" fontstyle="bold" fontsize="16">
        <label name="label2" x="0" y="12" width="100%" height="24" alignment="center">*** END OF REPORT ***</label>
    </reportfooter>

    <groups>
        <group name="Category">
            <groupheader height="24" fontname="SansSerif" fontstyle="bold" fontsize="12" pagebreak="false">
                <label name="label3" x="0" y="10" width="76" height="9" alignment="left">Category:</label>
                <string-field name="sf1" x="96" y="10" width="76" height="9" alignment="left" fieldname="Category"/>
                <line name="line1" x1="0" y1="23" x2="0" y2="23" weight="0.5"/>
            </groupheader>
            <groupfooter height="0" fontname="Monospaced" fontstyle="bold" fontsize="9">
            </groupfooter>
            <fields>
                 <field>Category</field>
            </fields>
        </group>
    </groups>

    <items height="14" fontname="SansSerif" fontstyle="plain" fontsize="10">
        <!-- <rectangle name="background" x="0" y="0" width="100%" height="100%" color="#DFDFDF" weight="0"/> -->
        <line name="top" x1="0" y1="0" x2="0" y2="0" color="#DFDFDF" weight="0.1"/>
        <line name="bottom" x1="0" y1="10" x2="0" y2="13" color="#DFDFDF" weight="0.1"/>
        <string-field name="sf3" x="0" y="2" width="76" height="10" alignment="left" fieldname="Name"/>
        <number-field name="sf4" x="260" y="2" width="76" height="10" alignment="right" format="#,##0" fieldname="Size"/>
        <image-field name="field99" x="170" y="1" width="12" height="12" fieldname="Icon" />
    </items>

    <functions>
      <function name="report_date" class="com.jrefinery.report.function.ReportPropertyFunction">
        <properties>
          <property name="reportProperty">report.date</property>
        </properties>
      </function>
    </functions>
</report>
